function create_spawner()

 local ent = {}
 ent.type = "spawner"
 ent.spawn_counter = 0
 ent.spawn_counter_limit = 15
 
 ent.draw = function()
  
 end

 ent.update = function()
 
  for event in all(events) do
   if (event == "moved") then
    ent.spawn_counter += 1
   end
  end
  
  if (ent.spawn_counter >= 
      ent.spawn_counter_limit) then
      
   ent.spawn_counter = 0
   
   local player = get_player()
   
   local enemy_x = player.x
   local enemy_y = player.y
   
   if (rnd(100)<50) then
    enemy_x -= 120
   else
    enemy_x += 120
   end
   
   if (rnd(100)<50) then
    enemy_y -= 120
   else
    enemy_y += 120
   end
   
   add(ents, create_enemy(enemy_x, enemy_y))
    
  end
  
 end 
 
 return ent

end


