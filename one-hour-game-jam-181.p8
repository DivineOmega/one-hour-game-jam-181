ents = {}
events = {}
game_over = false

function _init()

 ents = {}
 events = {}
 game_over = false

 add(ents, create_player())
 add(ents, create_enemy(115, 115))
 add(ents, create_spawner())
 
end

function _update()

 if (game_over == true) then
 	if (btnp(5)) then
 		_init()
 	end
  return
 end

 for ent in all(ents) do
  ent.update()
 end
 
 events = {}
 
end

function _draw()

 rectfill(0, 0, 127, 127, 0)
 rect(0, 0, 125, 125, 5)
 
 for ent in all(ents) do
  ent.draw()
 end
 
 if (game_over == true) then
  print("game over!", 45, 60, 8)
  print("press âŽ when ready", 25, 70, 6)
 end
 
end

function get_player()
 for ent in all(ents) do
  if (ent.type == "player") then
   return ent
  end
 end
end

